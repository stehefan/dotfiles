# Aliases for git
alias ga="git add"
alias gst="git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias gpr="git pull -r"
alias gs="git switch"
alias gsm="gs master"
alias gfp="git fetch -p"
alias gca="git commit --amend"
alias gl="git log --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'"
alias gc="git commit"
alias gpuo="git push -u origin"
alias gpo="git push origin"
